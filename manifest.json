{
  "manifest_version": 2,
  "name": "LeetcodeShare",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://leetcode.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "popup.js",
    "popup.html",
    "content.js",
    "background.js",
    "hello_extensions.png"
  ]
}
